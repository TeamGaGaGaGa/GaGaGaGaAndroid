<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- selector가 눌렸을때 @drawable/button01_press 를 보여준다.-->
    <item android:state_selected="false" android:color="@color/black"/>

    <!-- selector가 눌리지 않았을때 @drawable/button01_normal 를 보여준다.-->
    <item android:state_selected="true" android:color="@color/main_green"/>

</selector>